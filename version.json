{
  "version": "1.2.0",
  "nugetPackageVersion": {
    "semVer": 1
  },
  "semVer1NumericIdentifierPadding": 4,
  "publicReleaseRefSpec": [
    "^refs/heads/release",
    "^refs/tags/\\d+\\.\\d+(?:\\.\\d+)?"
  ],
  "cloudBuild": {
    "setVersionVariables": true,
    "buildNumber": {
      "enabled": false,
      "includeCommitId": {
        "when": "nonPublicReleaseOnly",
        "where": "buildMetadata"
      }
    }
  }
}
